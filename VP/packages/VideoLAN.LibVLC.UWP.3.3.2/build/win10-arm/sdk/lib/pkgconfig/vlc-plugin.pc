prefix=/builds/videolan/libvlc-nuget/vlc/winarm-uwp/_win32
exec_prefix=${prefix}
includedir=${prefix}/include
datarootdir=${prefix}/share
libdir=${exec_prefix}/lib
datadir=${datarootdir}
pkgincludedir=${prefix}/include/vlc
pkgdatadir=${datadir}/vlc
pkglibdir=${libdir}/vlc
pluginsdir=${pkglibdir}/plugins

Name: VLC plugin API
Description: VLC media player plugin interface
Version: 3.0.16
Cflags: -I${includedir} -I${pkgincludedir}/plugins \
	-D__PLUGIN__ \
	-D_FILE_OFFSET_BITS=64 \
	 \
	-D_REENTRANT \
	-D_THREAD_SAFE
Libs: -L${libdir} -lvlccore
Libs.private: -lruntimeobject -lsynchronization  \
	 -L/builds/videolan/libvlc-nuget/vlc/contrib/armv7-w64-mingw32uwp/lib -liconv -lwindowsapp -lwindowsappcompat -lvcruntime140_app -lucrt \
	-lws2_32 -liphlpapi  -lm
